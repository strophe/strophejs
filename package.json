{
  "name": "strophe.browseryfy.js",
  "description": "Strophe.js is an XMPP library for JavaScript",
  "version": "1.2.14",
  "homepage": "http://strophe.im/strophejs",
  "repository": {
    "type": "git",
    "url": "git://github.com/strophe/strophejs.git"
  },
  "keywords": [
    "xmpp",
    "message",
    "bosh",
    "websocket",
    "browser"
  ],
  "files": [
    "strophe.js",
    "CHANGELOG.txt",
    "LICENSE.txt",
    "README.txt",
    "src/"
  ],
  "author": "Jack Moffit (metajack)",
  "contributors": [
    "Nathan Zorn (thepug)",
    "Andreas Guth (Gordin)",
    "Anton Stroganov (Aeon)",
    "Florian Zeitz (Florob)",
    "Christopher Zorn (twonds)",
    "dodo",
    "Lee Boynton (lboynton)",
    "Theo Cushion (theozaurus)",
    "Brendon Crawford (brendoncrawford)",
    "JC Brand (jcbrand)",
    "Saeed Seyfi (saeedseyfi)"
  ],
  "license": "MIT",
  "main": "strophe.browserify.js",
  "browser": {
    "sinon": "./node_modules/sinon/lib/sinon.js",
    "sinon-qunit": "./node_modules/sinon-qunit/lib/sinon-qunit.js"
  },
  "browserify-shim": {
    "sinon-qunit": {
      "depends": [
        "sinon:sinon"
      ]
    }
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "scripts": {
    "lint": "make jshint",
    "clean": "make clean",
    "doc": "make doc"
  },
  "engines": {
    "browser": "*"
  },
  "volo": {
    "url": "https://raw.githubusercontent.com/strophe/strophejs/release-{version}/strophe.js"
  },
  "devDependencies": {
    "browserify": "^14.5.0",
    "browserify-shim": "^3.8.14",
    "http-server": "^0.10.0",
    "jquery": "3.2.1",
    "jshint": "^2.9.4",
    "phantomjs": "~1.9.7-1",
    "qunit-phantomjs-runner": "^2.1.0",
    "qunitjs": "~1.16.0",
    "sinon": "1.17.7",
    "sinon-qunit": "~2.0.0",
    "uglify-js": "^3.1.6"
  }
}
