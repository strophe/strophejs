    "babel-eslint": "^10.1.0",
    "es6-promise": "^4.2.8",
    "eslint": "4.19.1",
    "http-server": "^14.1.0",
    "karma": "^6.3.17",
    "karma-chrome-launcher": "^3.1.1",
    "karma-qunit": "^4.1.2",
